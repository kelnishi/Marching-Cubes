static const int numThreads = 8;

RWStructuredBuffer<float> points;
int3 dimensions;
float3 worldSize;
float2 densityRange;

float boundsSize;
float3 center;
float3 offset;
float3 spacing;

int indexFromCoord(int3 coord)
{
    return ((coord.z * dimensions.z) + coord.y) * dimensions.y + coord.x;
}

float3 positionOfCoord(int3 coord)
{
    return ((float3)coord/dimensions + offset) * boundsSize;
}